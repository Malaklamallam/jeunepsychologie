@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vs: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix o: <http://omeka.org/s/vocabs/o#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix asp: <https://acces-soins-psychologiques93.fr/vocab#> .

<https://acces-soins-psychologiques93.fr/vocab#> a owl:Ontology ;
    dcterms:title "Vocabulaire Accès Soins Jeunes (93)" ;
    dcterms:description "Vocabulaire commun pour la gestion du parcours de soins psychologiques des jeunes en Seine-Saint-Denis." .


### --- CLASSES ---

asp:Youth a rdfs:Class ;
    rdfs:label "Jeune / Patient" ;
    rdfs:comment "Représente le jeune bénéficiaire des soins." ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

asp:SchoolInstitution a rdfs:Class ;
    rdfs:label "Établissement Scolaire" ;
    rdfs:comment "Lieu de scolarisation et de détection." ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

asp:HealthProfessional a rdfs:Class ;
    rdfs:label "Professionnel de Santé" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

asp:Psychologist a rdfs:Class ;
    rdfs:label "Psychologue" ;
    rdfs:subClassOf asp:HealthProfessional ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

asp:Psychiatrist a rdfs:Class ;
    rdfs:label "Psychiatre" ;
    rdfs:subClassOf asp:HealthProfessional ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

asp:MedicoSocialStructure a rdfs:Class ;
    rdfs:label "Structure Médico-Sociale" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

asp:CMP a rdfs:Class ;
    rdfs:label "CMP (Centre Médico-Psychologique)" ;
    rdfs:subClassOf asp:MedicoSocialStructure ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

asp:MDA a rdfs:Class ;
    rdfs:label "MDA (Maison des Adolescents)" ;
    rdfs:subClassOf asp:MedicoSocialStructure ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

asp:Hospital a rdfs:Class ;
    rdfs:label "Hôpital" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

asp:PsychologicalProgram a rdfs:Class ;
    rdfs:label "Programme Psychologique" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

asp:ARS_IDF a rdfs:Class ;
    rdfs:label "ARS Île-de-France" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

asp:LocalAuthority a rdfs:Class ;
    rdfs:label "Collectivité Locale" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

asp:LocalAssociation a rdfs:Class ;
    rdfs:label "Association Locale" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .


asp:FollowUpFile a rdfs:Class ;
    rdfs:label "Dossier de Suivi" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

asp:Consultation a rdfs:Class ;
    rdfs:label "Consultation" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

asp:Referral a rdfs:Class ;
    rdfs:label "Orientation" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .


